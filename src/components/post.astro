---
import type { CollectionEntry } from 'astro:content';


type Props = {
    artwork: CollectionEntry<"artworks">;
}

const { artwork } = Astro.props;
const { title, year } = artwork.data;




---
<a href={`/works/${year}/${artwork.id}`} class="item2">
    <img src={`/artwork/${title}/main.jpg`} alt={title} class="item" />
    <div class="overlay">{title}</div>
</a>